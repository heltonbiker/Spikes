<Window x:Class="GerenciadorDeColeta.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow"
		WindowState="Maximized"
		Height="350" Width="525">
    <Grid>
    <ContentControl x:Name="conteudo_janela">
		<StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
			<TextBox Text="Vert3D" HorizontalAlignment="Center" VerticalAlignment="Center" Height="70" TextAlignment="Center" FontSize="48" />
			<TextBox Text="Esta é a tela de exame, e o botão abaixo é o botão pra iniciar a coleta" HorizontalAlignment="Center" VerticalAlignment="Center" Height="70" TextAlignment="Center" />
			<Button Content="Iniciar Coleta" Click="iniciar_coleta" x:Name="coletabutton" Width="300" Height="100"/>
		</StackPanel>
	</ContentControl>
    </Grid>
</Window>
