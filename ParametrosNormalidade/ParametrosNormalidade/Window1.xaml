<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:ParametrosNormalidade"
	x:Class="ParametrosNormalidade.Window1"
	x:Name="Window"
	Title="Window1"
	Width="700" Height="480" Background="#FF868D44">

	<Grid x:Name="LayoutRoot">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="0.062*"/>
			<ColumnDefinition Width="0.262*"/>
			<ColumnDefinition Width="0.355*"/>
			<ColumnDefinition Width="0.321*"/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="0.506*"/>
			<RowDefinition Height="0.224*"/>
			<RowDefinition Height="0.27*"/>
		</Grid.RowDefinitions>
		
		<DockPanel x:Name="dock" Grid.Column="2" Background="#FFE2ABAB" Grid.Row="1">
		
			<local:Notes Panel.ZIndex="99" DockPanel.Dock="Bottom"/>
		</DockPanel>
		
	</Grid>
</Window>